{
  "name": "@meistrari/n8n-nodes-tela",
  "version": "1.0.0",
  "description": "n8n community node for Tela API integration. Allows you to interact with Tela projects and canvases in your n8n workflows.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && npm run build:icons",
    "prepublishOnly": "npm run build",
    "build:icons": "gulp build:icons"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      {
        "name": "Tela",
        "type": "n8n-nodes-tela.Tela",
        "file": "dist/Tela.node.js",
        "icon": "file:tela.png"
      }
    ],
    "credentials": [
      {
        "name": "telaApi",
        "type": "n8n-nodes-tela.telaApi",
        "file": "dist/Tela.credentials.js",
        "icon": "file:tela.png"
      }
    ]
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE.md"
  ],
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "tela",
    "api",
    "integration",
    "workflow",
    "automation"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meistrari/n8n-nodes-tela.git"
  },
  "homepage": "https://github.com/meistrari/n8n-nodes-tela#readme",
  "bugs": {
    "url": "https://github.com/meistrari/n8n-nodes-tela/issues"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "gulp": "^5.0.1",
    "n8n-workflow": "^1.0.0",
    "typescript": "^4.8.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
